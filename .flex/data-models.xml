<?xml version="1.0" encoding="UTF-8"?>

<entities namespace="Foo\Data" target="../src">
    <entity class="User">
        <fields>
            <field name="id" />
            <field name="name" phpMethod="myName" phpType="string" />
            <field name="created_at" phpType="DateTime" phpTypeHinting="true" />
        </fields>
    </entity>

    <entity class="User\Payment\Transaction">
        <fields>
            <field name="id" phpType="int" />
            <field name="transaction_id" phpType="string" />
            <field name="created_at" phpType="DateTime" phpTypeHinting="true" />
        </fields>
    </entity>
</entities>